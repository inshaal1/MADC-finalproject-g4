{
  "expo": {
    "name": "expoclass",
    "slug": "expoclass",
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/app-icon.png",  
    "splash": {
      "image": "./assets/qrcode.png",
      "resizeMode": "contain",
      "backgroundColor": "#333333"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": ["CAMERA"], 
      "package": "com.expoclass.yourapp"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
